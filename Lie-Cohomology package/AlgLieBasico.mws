</Output>
</Group>
<Group labelreference="L127" drawlabel="true" applyint="true" applyrational="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">sl:=proc(n) local L1,L2,L3,i,j,A:
L1:=[]:
L2:=[]:
L3:=[]:
for i from n to 1 by -1 do:
  for j from i+1 to n do:
    A:=Matrix(n,n):
    A[j,j-i]:=1:
    L3:=[A,op(L3)]:
    L1:=[op(L1),transpose(A)]:
  od:
od:
for i from n to 2 by -1 do:
  A:=Matrix(n,n):
  A[n+1-i,n+1-i]:=1:
  A[n+2-i,n+2-i]:=-1:
  L2:=[op(L2),A]:
od:
[op(L1),op(L2),op(L3)]:
end proc:  </Text-field>
</Input>
</Group>
