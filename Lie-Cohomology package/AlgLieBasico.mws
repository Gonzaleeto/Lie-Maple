<Group labelreference="L153" drawlabel="true" applyint="true" applyrational="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">IdealGenerado:=proc(L,T) local B,i,j,l1,l2,L1,dim,l,v:

dim:=nops(T[1,2]):
v:=[seq(0,l=1..dim)]:

L1:=BaseListas(L):

l1:=nops(L1):
l2:=0:

while l1&gt;l2 do:
B:=L1:
  for i from l2+1 to l1 do:
    for j from 1 to dim do:
      B:=[op(B),LieAbs(L1[i],subsop(j=1,v),T)]:
    od:
  od:
  L1:=BaseListas(B):
  l2:=l1:
  l1:=nops(L1):
od:
  
L1:
end proc:</Text-field>
