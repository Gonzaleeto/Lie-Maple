<Group labelreference="L205" drawlabel="true" applyint="true" applyrational="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">DerivacionesInt:=proc(T) local k,i,j,n,M,A:
n:=nops(T[1,2]):
for k from 1 to n do:
  A[k]:=matrix(n,n,0):
  for i from 1 to n do:
  for j from 1 to n do:
    if j&lt;k then A[k][i,j]:=-T[j,k][i]:fi:
    if j&gt;k then A[k][i,j]:=T[k,j][i]:fi:
   od:
   od:
od:
A:=BaseMat(convert(A,list)):
print(&quot;Dim=&quot;,nops(A)):
M:=matrix(n,n,0):
for i from 1 to nops(A) do:
   M:=evalm(M+z||i*A[i]):
od:
evalm(M):
end proc:</Text-field>
</Input>
</Group>
