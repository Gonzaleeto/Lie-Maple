sgM13 := sgM12.sgM23.sgM12:
   
II := sgM12^2:

sgM123 := sgM12.sgM23:
sgM132 := sgM23.sgM12:

sgSt := [II,sgM12,sgM23,sgM13,sgM123,sgM132];
</Text-field>
