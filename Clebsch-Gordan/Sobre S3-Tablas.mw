T_FS3[i,4][5]:=1: T_FS3[4,i][6]:=1:
T_FS3[i,5][4]:=1: T_FS3[5,i][2]:=1:
T_FS3[i,6][2]:=1: T_FS3[6,i][4]:=1:

i:=4:
