T_FS3[i,3][5]:=1: T_FS3[3,i][6]:=1:
T_FS3[i,4][6]:=1: T_FS3[4,i][5]:=1:
T_FS3[i,5][3]:=1: T_FS3[5,i][4]:=1:
T_FS3[i,6][4]:=1: T_FS3[6,i][3]:=1:

i:=3:
