<Group view="code" hide-input="false" hide-output="false" labelreference="L3602" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font foreground="[255,153,0]"># Tabla de multiplicar de F[S3] en la base canonica {e1,e12,e23,e13,e123,e132}</Font><Font foreground="[255,204,0]">
</Font><Font foreground="[128,0,0]"> 
T_FS3:='T_FS3':

for i from 1 to 6 do:
   for j from 1 to 6 do:
      T_FS3[i,j]:=[seq(0,k=1..6)]:
   od:
od:

T_FS3[1,1][1]:=1:
T_FS3[1,2][2]:=1: T_FS3[2,1][2]:=1:
T_FS3[1,3][3]:=1: T_FS3[3,1][3]</Font>:=1:
T_FS3[1,4][4]:=1: T_FS3[4,1][4]:=1:
T_FS3[1,5][5]:=1: T_FS3[5,1][5]:=1:
T_FS3[1,6][6]:=1: T_FS3[6,1][6]:=1:

i:=2:
